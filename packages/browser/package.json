{
  "name": "@figtreejs/browser",
  "version": "0.0.0",
  "description": "A standalone library for phylogenetic visualization",
  "repository": {
    "type": "git",
    "url": "https://github.com/figtreo/figtreejs.git"
  },
  "license": "MIT",
  "author": "Andrew Rambaut and JT McCrone",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "vite build",
    "check-types": "tsc --noEmit ",
    "dev": "vite",
    "lint": "eslint src/"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write --ignore-unknown"
    ],
    "!(*.js|*.ts)": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@figtreejs/core": "*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@figtreejs/typescript-config": "*",
    "@figtreejs/eslint-config": "*",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.8"
  }
}
