{
  "name": "@figtreejs/core",
  "version": "0.0.1",
  "description": "A react component library for phylogenetic visualization and app building",
  "repository": {
    "type": "git",
    "url": "https://github.com/figtreo/figtreejs.git"
  },
  "license": "MIT",
  "author": "Andrew Rambaut and JT McCrone",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "vite build",
    "check-types": "tsc --noEmit",
    "dev": "vite build --watch",
    "lint": "eslint src/",
    "test": "vitest",
    "vitest-preview": "vitest-preview"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write --ignore-unknown"
    ],
    "!(*.js|*.ts)": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@react-spring/web": "^9.7.5",
    "abs-svg-path": "^0.1.1",
    "d3-array": "^3.2.4",
    "d3-format": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "d3-time-format": "^4.1.0",
    "immer": "^10.1.1",
    "normalize-svg-path": "^1.1.0",
    "parse-svg-path": "^0.1.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@figtreejs/eslint-config": "*",
    "@figtreejs/maybe": "*",
    "@figtreejs/typescript-config": "*",
    "@testing-library/react": "^16.3.0",
    "@types/abs-svg-path": "^0.1.3",
    "@types/d3-array": "^3.2.2",
    "@types/d3-format": "^3.0.4",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-time-format": "^4.0.3",
    "@vitejs/plugin-react": "^5.1.0",
    "jsdom": "^27.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.8"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
