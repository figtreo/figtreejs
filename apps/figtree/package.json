{
  "name": "@figtreejs/figtree",
  "version": "0.0.1",
  "type": "module",
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "!(*.js|*.ts)": [
      "prettier --write --ignore-unknown"
    ]
  },
  "scripts": {
    "dev": "vite --port 8080",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "format": "prettier --write .",
    "lint": "eslint ./src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@figtreejs/core": "^0.0.1",
    "@reduxjs/toolkit": "^1.9.7",
    "d3-format": "^3.1.0",
    "d3-selection": "^3.0.0",
    "d3-time-format": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^8.1.3",
    "redux-undo": "^1.1.0"
  },
  "devDependencies": {
   
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],

    "rules": {

      "react/jsx-no-target-blank": "off"
    }
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/figtreo/figtreejs.git"
  }
}
